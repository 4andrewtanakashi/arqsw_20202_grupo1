{

	"Pacotes": {
		"cadastropessoas": ["CadastropessoaApplication"],
		"controller": ["PessoasControl"],
		"models": ["Pessoa"],
		"repository": ["PessoaRepository"],
		"service": [ "PessoasService"],
		"EXTERNAL": ["CrudRepository", "RestController"]
	},

	"LigacoesDePacotes": {
		"controller": {
			"Pode": ["models"],
	    	"Deve": ["service", "RestController"]
		},

		"repository": {
			"Deve": ["CrudRepository", "models"]
		},

		"service": {
	    	"Deve": ["repository", "models"]
		}

	}

}
